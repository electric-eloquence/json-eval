{
  "name": "json-eval",
  "version": "0.2.0",
  "description": "Evaluate a stringfied JS object into JSON without eval()",
  "main": "src/index.js",
  "scripts": {
    "bundle": "node run/bundle-client.js",
    "devel": "npm run watch & npm run serve",
    "lint": "npx eslint src && npx eslint test",
    "prepush": "npm run lint && npm test",
    "serve": "npx http-server .",
    "test": "npx mocha test/tests.js",
    "watch": "npx nodemon --watch src -e js --exec 'npm run bundle'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electric-eloquence/json-eval.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/electric-eloquence/json-eval/issues"
  },
  "homepage": "https://github.com/electric-eloquence/json-eval#readme",
  "dependencies": {
    "json5": "^0.5.0"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "chai": "4.x",
    "eslint": "3.x",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "mocha": "3.x",
    "nodemon": "^1.14.12",
    "uglify-es": "3.3.9"
  }
}
