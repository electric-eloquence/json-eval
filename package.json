{
  "name": "json-eval",
  "version": "1.0.8",
  "description": "Parse JSON-like strings into valid JS objects without eval",
  "main": "src/index.js",
  "scripts": {
    "bundle": "node run/bundle-client.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src && eslint test",
    "serve": "node run/serve",
    "start": "npm run watch && npm run serve",
    "test": "nyc mocha test/{amd,main}.js",
    "watch": "nodemon --watch src -e js --exec 'npm run bundle'",
    "wdio": "node test/wdio-es5 && node test/wdio-es6.js"
  },
  "husky": {
    "hooks": {
      "post-commit": "npm run bundle",
      "pre-push": "npm run lint && npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electric-eloquence/json-eval.git"
  },
  "keywords": [
    "eval",
    "json",
    "parse"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/electric-eloquence/json-eval/issues"
  },
  "homepage": "https://github.com/electric-eloquence/json-eval#readme",
  "dependencies": {
    "json5": "0.5.1"
  },
  "devDependencies": {
    "@wdio/cli": "^6.0.15",
    "@wdio/local-runner": "^6.0.15",
    "@wdio/mocha-framework": "^6.0.15",
    "@wdio/spec-reporter": "^6.0.14",
    "@wdio/sync": "^6.0.15",
    "browserify": "^16.2.3",
    "chai": "4.x",
    "chai-webdriverio": "^1.0.0",
    "chromedriver": "^80.0.2",
    "coveralls": "3.x",
    "eslint": "6.x",
    "husky": "4.x",
    "mocha": "7.x",
    "nodemon": "^1.18.10",
    "nyc": "15.x",
    "uglify-es": "3.3.9",
    "wdio-chromedriver-service": "^6.0.1",
    "webdriverio": "^6.0.15"
  }
}
